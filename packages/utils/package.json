{
  "name": "favorite_box",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "https://github.com/jzendo/favorite_box.git",
  "author": "zendo <jzendo@example.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:cjs",
    "build:cjs": "babel --no-comments src --out-dir build_cjs --ignore \"src/**/__tests__/**\"",
    "build:optOutput": "babel --plugins minify-dead-code-elimination --no-comments build_cjs --out-dir build_cjs --ignore \"src/**/__tests__/**\"",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "jest"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "node_modules",
      "build",
      "build_cjs"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-minify-constant-folding": "^0.5.0",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "cross-env": "^7.0.1",
    "jest": "^25.1.0",
    "rollup": "^1.31.0",
    "standard": "^14.3.1"
  },
  "dependencies": {
    "get-parameter-names": "^0.3.0"
  }
}
